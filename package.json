{
  "name": "openbazaar",
  "version": "0.1.1",
  "private": true,
  "author": "Zababurin Sergey <s.zababurin.v@gmail.com>",
  "license": "Unlicense",
  "scripts": {
    "alise": "npm-run-all -s -c kill_alise start_alise",
    "bob": "npm-run-all -s -c kill_bob start_bob ",
    "node1": "npm-run-all -s -c kill_zb start_zb ",
    "node2": "npm-run-all -s -c kill_olga start_olga ",
    "start_alise": "cross-env REACT_APP_PROVIDER_SOCKET=ws://127.0.0.1:9945 PORT=8225 react-app-rewired start",
    "start_bob": "cross-env REACT_APP_PROVIDER_SOCKET=ws://127.0.0.1:9946 PORT=8226 react-app-rewired start",
    "start_node1": "cross-env REACT_APP_PROVIDER_SOCKET=ws://127.0.0.1:9947 PORT=8227 react-app-rewired start",
    "start_node2": "cross-env REACT_APP_PROVIDER_SOCKET=ws://127.0.0.1:9948 PORT=8228 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "",
    "eject": "react-app-rewired eject",
    "lint": "eslint src/**/*.js src/**/*.mjs",
    "lint:ci": "eslint src/**/*.js src/**/*.mjs --max-warnings=0",
    "lint:fix": "eslint --fix src/**/*.js src/**/*.mjs",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'",
    "kill_alise": "kill $(lsof -t -i:8221)",
    "kill_bob": "kill $(lsof -t -i:8222)",
    "kill_zb": "kill $(lsof -t -i:8223)",
    "kill_olga": "kill $(lsof -t -i:8224)"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.14.4",
    "@polkadot/api": "^4.12.1",
    "@polkadot/extension-dapp": "^0.38.3",
    "@polkadot/keyring": "^6.6.1",
    "@polkadot/networks": "^6.6.1",
    "@polkadot/types": "^4.12.1",
    "@polkadot/ui-keyring": "^0.78.1",
    "@polkadot/ui-settings": "^0.78.1",
    "@polkadot/util": "^6.6.1",
    "@polkadot/util-crypto": "^6.6.1",
    "@reduxjs/toolkit": "^1.6.0",
    "babel-eslint": "^10.1.0",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "standard": "^16.0.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.8",
    "semistandard": "^16.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "semistandard"
    ],
    "plugins": [
      "only-warn"
    ]
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://substrate-developer-hub.github.io/substrate-front-end-template",
  "bugs": {
    "url": "https://github.com/substrate-developer-hub/substrate-front-end-template/issues"
  },
  "keywords": [
    "substrate",
    "substrate-ui",
    "polkadot-js"
  ],
  "standard": {
    "parser": "babel-eslint"
  }
}
